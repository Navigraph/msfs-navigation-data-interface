{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "declaration": true,
    "noEmit": true,
    "strictNullChecks": true
  },
  "include": ["**/*.ts"],
  "exclude": ["**/node_modules"]
}
